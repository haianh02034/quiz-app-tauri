{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "quiz-app-fic",
  "version": "0.1.0",
  "identifier": "com.quiz-app.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "TRẮC NGHIỆM “BẠN BIẾT GÌ VỀ CÔNG NGHỆ?",
        "width": 1550,
        "height": 1008,
        "resizable": false,
        "fullscreen": false,
        "devtools": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": [
      "../../quiz.json"
    ]
  }
}
